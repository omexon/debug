{
    "name": "omexon/debug",
    "type": "library",
    "description": "Various helpers for debugging code",
    "license": "MIT",
    "keywords": [
        "debug"
    ],
    "require": {
        "php": "^7.2",
        "symfony/var-dumper": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Omexon\\Debug\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Omexon\\Debug\\": "tests"
        }
    }
}
